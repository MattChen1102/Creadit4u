<%= simple_form_for([:admin, @order]) do |f| %>
  <div class="form-group">
    <%= f.input :name %>
  </div>
  <div class="form-group">
    <%= f.input :email %>
  </div>
  <div class="form-group">
    <%= f.input :mobile %>
  </div>

  <div class="form-group">
    <%= f.input :paid %>  
  </div>

  <div class="form-actions">
    <%= f.button :submit ,class: 'btn btn-primary' %>
  </div>
<% end %>
<hr>
<br>